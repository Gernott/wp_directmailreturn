			"homepage": "https://www.webprofil.at/"
		},
		{
			"name": "Reelworx GmbH",
			"role": "Developer",
			"homepage": "https://reelworx.at/"
		},
		{
			"name": "JÃ¶rg Velletti",
			"role": "Developer",
			"homepage": "http://www.velletti.de/"
		}
	],
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^7.6 || ^8.7 || ^9.5",
		"ext-imap": "*"
	},
	"replace": {
		"typo3-ter/wp_directmailreturn": "self.version"
	},
